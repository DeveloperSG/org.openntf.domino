<?xml version='1.0' encoding='UTF-8'?>
<schema targetNamespace="org.openntf.domino.rest" xmlns="http://www.w3.org/2001/XMLSchema">
<annotation>
      <appinfo>
         <meta.schema plugin="org.openntf.domino.rest" id="org.openntf.domino.rest.graph" name="org.openntf.domino.rest.graph"/>
      </appinfo>
      <documentation>
         This extension point can be used to add new graphs to the OpenNTF Domino REST service or add additional Resource classes to handle extended Routes and Methods
      </documentation>
   </annotation>

   <element name="extension">
      <annotation>
         <appinfo>
            <meta.element />
         </appinfo>
      </annotation>
      <complexType>
         <choice>
            <element ref="graphFactory"/>
         </choice>
         <choice>
            <element ref="resourceProvider"/>
         </choice>
         <attribute name="point" type="string" use="required">
            <annotation>
               <documentation>
                  
               </documentation>
            </annotation>
         </attribute>
         <attribute name="id" type="string">
            <annotation>
               <documentation>
                  
               </documentation>
            </annotation>
         </attribute>
         <attribute name="name" type="string">
            <annotation>
               <documentation>
                  
               </documentation>
            </annotation>
         </attribute>
      </complexType>
   </element>

   <element name="graphFactory">
      <complexType>
         <attribute name="class" type="string" use="required">
            <annotation>
               <documentation>
                  
               </documentation>
               <appinfo>
                  <meta.attribute kind="java" basedOn=":org.openntf.domino.rest.service.IGraphFactory"/>
               </appinfo>
            </annotation>
         </attribute>
      </complexType>
   </element>
   
   <element name="resourceProvider">
      <complexType>
         <attribute name="class" type="string" use="required">
            <annotation>
               <documentation>
                  
               </documentation>
               <appinfo>
                  <meta.attribute kind="java" basedOn=":org.openntf.domino.rest.service.IResourceProvider"/>
               </appinfo>
            </annotation>
         </attribute>
      </complexType>
   </element>
</schema>
